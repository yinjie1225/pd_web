export default {
  namespaced: true,
  state: {
    id: 0,
    name: '',
    comcode: ''
  },
  mutations: {
    updateId (state, id) {
      state.id = id
    },
    updateName (state, name) {
      state.name = name
    },
    updateCode (state, comcode) {
      state.comcode = comcode
    }
  }
}
